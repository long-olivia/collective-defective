      "**/*.so": true,
      "**/*.afm": true,
      "**/*.ttf": true,
      "**/*.json": true,
      "**/*.csv": true
    },
    "files.exclude": {
      "**/*.pyc": true,
      "**/__pycache__": true,
      "**/*.so": true,
      "**/*.afm": true,
      "**/*.ttf": true,
      "**/node_modules": true,
      "**/data": true,
      "**/.git": true
    },
    "search.exclude": {
      "**/*.pyc": true,
      "**/__pycache__": true,
      "**/*.so": true,
      "**/*.afm": true,
      "**/*.ttf": true,
      "**/*.json": true,
      "**/*.csv": true
    },
    "python.analysis.exclude": [
      "**/__pycache__",
      "**/*.pyc"
